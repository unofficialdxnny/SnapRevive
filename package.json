{
  "name": "snapify",
  "version": "6.0.0",
  "description": "Snapify Boost Snapscore",
  "main": "main.js",
  "scripts": {
    "start": "electron .",
    "package": "electron-builder"
  },
  "repository": "https://github.com/unofficialdxnny/Snapify",
  "keywords": [
    "Electron",
    "quick",
    "start",
    "tutorial",
    "demo"
  ],
  "author": "unofficialdxnny (Danial Ahmed)",
  "license": "CC0-1.0",
  "devDependencies": {
    "electron": "^26.1.0",
    "electron-builder": "^24.13.3"
  },
  "dependencies": {
    "chromedriver": "^116.0.0",
    "codeceptjs": "^3.5.4",
    "discord-rpc": "^4.0.1",
    "electron-reload": "^2.0.0-alpha.1",
    "express": "^4.19.2",
    "puppeteer": "^21.1.1",
    "selenium-webdriver": "^4.12.0",
    "webdriverio": "^8.16.4"
  },
  "build": {
    "appId": "com.example.snapify",
    "productName": "Snapify",
    "directories": {
      "output": "release-builds"
    },
    "win": {
      "target": "portable",
      "icon": "icons/builder-logo.ico"
    },
    "mac": {
      "target": "dmg",
      "icon": "icons/icon.icns"
    },
    "linux": {
      "target": "AppImage",
      "icon": "icons/icon.png"
    },
    "files": [
      "main.js",
      "renderer.js",
      "icons/**/*",
      "node_modules/",
      "validate.html",
      "index.html"
    ]
  }
}